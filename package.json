{
  "name": "@scent/template",
  "version": "1.0.7",
  "description": "",
  "main": "dist/scent.template.js",
  "mainMini": "dist/scent.template.mini.js",
  "module": "dist/scent.template.es.js",
  "bin": "bin/scent.template.js",
  "libName": "scent.template",
  "_sourceName": "scent.template",
  "author": "Cangshi",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn run rollup && yarn run rollup:mini && yarn run rollup:esm:es5",
    "do-publish": "yarn run build && yarn run run-publish ",
    "run-publish": "yarn publish --access public --tag latest --new-version $npm_package_version",
    "rollup": "rollup -c",
    "rollup:mini": "cross-env NODE_ENV=prod rollup -c",
    "rollup:esm": "cross-env NODE_ENV=esm rollup -c",
    "rollup:esm:es5": "cross-env NODE_ENV=esm5 rollup -c",
    "gen-dts": "npx tsc main/index.ts --declaration --emitDeclarationOnly --target ES2020 --moduleResolution node --outDir types",
    "gen-docs": "typedoc --out docs main/*.ts",
    "gen-dts:docs": "yarn run gen-dts && yarn run gen-docs"
  },
  "files": [
    "bin",
    "dist",
    "build",
    "docs",
    "types",
    "readme.md",
    "LICENSE",
    "package.json"
  ],
  "types": "types/index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.13.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^2.6.0",
    "rollup": "^2.45.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "typescript": "^4.1.2",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.1"
  },
  "repository": "https://github.com/canguser/scent.template.git"
}
